{
  "name": "tina-self-hosted-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TINA_PUBLIC_IS_LOCAL=true tinacms dev -c \"next dev\"",
    "dev:prod": "TINA_PUBLIC_IS_LOCAL=false tinacms dev -c \"next dev\"",
    "build": "tinacms build --partial-reindex && next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "format": "prettier . --write --cache",
    "format:check": "prettier . --check --cache"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "js-style-kit": "^0.4.2",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10"
  },
  "dependencies": {
    "@tinacms/cli": "^1.6.1",
    "@tinacms/datalayer": "^1.4.0",
    "next": "14.2.1",
    "next-auth": "^4.24.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tinacms": "^2.2.1",
    "tinacms-authjs": "^5.0.1",
    "tinacms-gitprovider-github": "^3.0.0",
    "typescript": "^5.5.4",
    "upstash-redis-level": "^1.1.1"
  }
}
